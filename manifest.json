{
  "manifest_version": 3,
  "name": "QuickTools",
  "description": "Provides a lot of useful tools",
  "version": "0.2",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "debugger",
    "tabs",
    "activeTab",
    "cookies",
    "tabGroups",
    "scripting",
    "storage",
    "contextMenus"
  ],
  "host_permissions": ["<all_urls>"]
}
